package week1.day2;

public class MissingNumber {

	public static void main(String[] args) {
		int[]n = {1,2,3,4,7,6,8};
		int sum=0;
		for (int i= 0;i< n.length;i++) {
			sum= sum+n[i];
		}
	

	int sum1=0;
     for(int j=1;j<=8;j++){
    	 sum1=sum1+j;
     }
     System.out.println("missing number"+(sum1-sum));
}
}
